{
  "name": "@ahoo-wang/fetcher-react",
  "version": "1.2.1",
  "description": "A modern, lightweight, and modular HTTP client based on the browser Fetch API, specially designed for LLM streaming APIs.",
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./storage": {
      "types": "./dist/storage/index.d.ts",
      "import": "./dist/storage/index.es.js",
      "require": "./dist/storage/index.umd.js"
    },
    "./wow": {
      "types": "./dist/wow/index.d.ts",
      "import": "./dist/wow/index.es.js",
      "require": "./dist/wow/index.umd.js"
    },
    "./fetcher": {
      "types": "./dist/fetcher/index.d.ts",
      "import": "./dist/fetcher/index.es.js",
      "require": "./dist/fetcher/index.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "keywords": [
    "fetch",
    "http",
    "client",
    "typescript",
    "llm",
    "stream",
    "sse",
    "react"
  ],
  "author": "ahoo wang <ahoowang@qq.com> (https://github.com/Ahoo-Wang)",
  "license": "Apache-2.0",
  "homepage": "https://github.com/Ahoo-Wang/fetcher#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ahoo-Wang/fetcher.git"
  },
  "bugs": {
    "url": "https://github.com/Ahoo-Wang/fetcher/issues"
  },
  "dependencies": {
    "@ahoo-wang/fetcher": "workspace:*",
    "@ahoo-wang/fetcher-storage": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@types/react": "^18.2.64",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "jsdom": "^22.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "5.3.3",
    "unplugin-dts": "^1.0.0-beta.6",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.3",
    "vitest": "^1.0.4"
  }
}