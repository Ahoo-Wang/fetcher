  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitest/coverage-v8": "^1.6.0",
    "jsdom": "^22.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
