{
  "name": "@ahoo-wang/fetcher",
  "version": "0.3.1",
  "description": "Core library providing basic HTTP client functionality for Fetcher",
  "keywords": [
    "fetch",
    "http",
    "client",
    "axios"
  ],
  "author": "Ahoo-Wang",
  "license": "Apache-2.0",
  "homepage": "https://github.com/Ahoo-Wang/fetcher/tree/master/packages/fetcher",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ahoo-Wang/fetcher.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/Ahoo-Wang/fetcher/issues"
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "test": "vitest run --coverage",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.33.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "unplugin-dts": "1.0.0-beta.6",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  }
}
