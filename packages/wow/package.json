{
  "name": "@ahoo-wang/fetcher-wow",
  "version": "1.8.0",
  "description": "Support for Wow(https://github.com/Ahoo-Wang/Wow) in Fetcher",
  "keywords": [
    "fetch",
    "http",
    "client",
    "wow",
    "cqrs",
    "ddd",
    "microservice",
    "event-sourcing"
  ],
  "author": "Ahoo-Wang",
  "license": "Apache-2.0",
  "homepage": "https://github.com/Ahoo-Wang/fetcher/tree/master/packages/wow",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ahoo-Wang/fetcher.git",
    "directory": "packages/wow"
  },
  "bugs": {
    "url": "https://github.com/Ahoo-Wang/fetcher/issues"
  },
  "type": "module",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "test": "vitest run --coverage",
    "lint": "eslint . --fix",
    "clean": "rm -rf dist",
    "analyze": "npx vite-bundle-analyzer -p auto"
  },
  "dependencies": {
    "@ahoo-wang/fetcher": "workspace:*",
    "@ahoo-wang/fetcher-eventstream": "workspace:*"
  },
  "devDependencies": {
    "typescript": "*",
    "eslint": "*",
    "@eslint/js": "*",
    "typescript-eslint": "*",
    "@vitest/coverage-v8": "*",
    "globals": "*",
    "prettier": "*",
    "vite": "*",
    "vitest": "*",
    "vite-bundle-analyzer": "*"
  }
}
