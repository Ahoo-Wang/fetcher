{
  "name": "@ahoo-wang/fetcher-wow",
  "version": "2.16.0",
  "description": "Support for Wow(https://github.com/Ahoo-Wang/Wow) in Fetcher",
  "keywords": [
    "fetch",
    "http",
    "client",
    "wow",
    "cqrs",
    "ddd",
    "microservice",
    "event-sourcing"
  ],
  "author": "Ahoo-Wang",
  "license": "Apache-2.0",
  "homepage": "https://github.com/Ahoo-Wang/fetcher/tree/master/packages/wow",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ahoo-Wang/fetcher.git",
    "directory": "packages/wow"
  },
  "bugs": {
    "url": "https://github.com/Ahoo-Wang/fetcher/issues"
  },
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./query/locale/zh_CN": {
      "types": "./dist/query/locale/zh_CN.d.ts",
      "import": "./dist/query/locale/zh_CN.es.js",
      "require": "./dist/query/locale/zh_CN.cjs.js"
    },
    "./query/locale/en_US": {
      "types": "./dist/query/locale/en_US.d.ts",
      "import": "./dist/query/locale/en_US.es.js",
      "require": "./dist/query/locale/en_US.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "test:ui": "vitest --ui",
    "test": "vitest run --coverage",
    "lint": "eslint . --fix",
    "clean": "rm -rf dist",
    "analyze": "npx vite-bundle-analyzer -p auto"
  },
  "peerDependencies": {
    "@ahoo-wang/fetcher": "workspace:^2.3.4",
    "@ahoo-wang/fetcher-eventstream": "workspace:^2.3.4",
    "@ahoo-wang/fetcher-decorator": "workspace:^2.3.4"
  },
  "devDependencies": {
    "@eslint/js": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "@vitest/ui": "catalog:",
    "eslint": "catalog:",
    "globals": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:",
    "unplugin-dts": "catalog:",
    "vite": "catalog:",
    "vite-bundle-analyzer": "catalog:",
    "vitest": "catalog:"
  }
}
