{
  "name": "@ahoo-wang/fetcher-eventstream",
  "version": "2.13.0",
  "description": "Server-Sent Events (SSE) support for Fetcher HTTP client with native LLM streaming API support. Enables real-time data streaming and token-by-token LLM response handling.",
  "keywords": [
    "fetch",
    "event-stream",
    "sse",
    "server-sent-events",
    "real-time",
    "streaming",
    "llm",
    "openai",
    "claude"
  ],
  "author": "Ahoo-Wang",
  "license": "Apache-2.0",
  "homepage": "https://github.com/Ahoo-Wang/fetcher/tree/master/packages/eventstream",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ahoo-Wang/fetcher.git",
    "directory": "packages/eventstream"
  },
  "bugs": {
    "url": "https://github.com/Ahoo-Wang/fetcher/issues"
  },
  "type": "module",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "test:ui": "vitest --ui",
    "test": "vitest run --coverage",
    "lint": "eslint . --fix",
    "clean": "rm -rf dist",
    "analyze": "npx vite-bundle-analyzer -p auto"
  },
  "peerDependencies": {
    "@ahoo-wang/fetcher": "workspace:^2.3.4"
  },
  "devDependencies": {
    "@eslint/js": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "@vitest/ui": "catalog:",
    "eslint": "catalog:",
    "globals": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:",
    "unplugin-dts": "catalog:",
    "vite": "catalog:",
    "vite-bundle-analyzer": "catalog:",
    "vitest": "catalog:"
  }
}
