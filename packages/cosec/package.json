  "homepage": "https://github.com/Ahoo-Wang/fetcher/tree/master/packages/cosec",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ahoo-Wang/fetcher.git",
    "directory": "packages/cosec"
  },
  "bugs": {
