{
  "name": "fetcher",
  "version": "1.8.0",
  "description": "A modern HTTP client library based on fetch API",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*",
    "integration-test"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "test:unit": "pnpm -r --filter=./packages/* test",
    "test:it": "pnpm -r --filter=./integration-test test",
    "lint": "pnpm -r lint",
    "format": "prettier --write .",
    "clean": "pnpm -r clean",
    "init": "sh ./scripts/init.sh",
    "update-version": "sh ./scripts/update-all-versions.sh"
  },
  "keywords": [
    "fetcher",
    "fetch"
  ],
  "author": "Ahoo-Wang",
  "license": "Apache-2.0",
  "homepage": "https://github.com/Ahoo-Wang/fetcher",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ahoo-Wang/fetcher.git"
  },
  "bugs": {
    "url": "https://github.com/Ahoo-Wang/fetcher/issues"
  },
  "packageManager": "pnpm@10.16.0",
  "devDependencies": {
    "typescript": "*",
    "eslint": "*",
    "@eslint/js": "*",
    "typescript-eslint": "*",
    "@vitest/coverage-v8": "*",
    "globals": "*",
    "prettier": "*",
    "unplugin-dts": "*",
    "vite": "*",
    "vitest": "*",
    "vite-bundle-analyzer": "*"
  }
}
