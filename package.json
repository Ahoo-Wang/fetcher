{
  "name": "fetcher",
  "version": "1.5.2",
  "description": "A modern HTTP client library based on fetch API",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*",
    "examples",
    "integration-test"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "test:unit": "pnpm -r --filter=./packages/* test",
    "test:it": "pnpm -r --filter=./integration-test test",
    "lint": "pnpm -r lint",
    "format": "prettier --write .",
    "clean": "pnpm -r clean",
    "init": "sh ./scripts/init.sh",
    "update-version": "sh ./scripts/update-all-versions.sh"
  },
  "keywords": [
    "fetcher",
    "fetch"
  ],
  "author": "Ahoo-Wang",
  "license": "Apache-2.0",
  "homepage": "https://github.com/Ahoo-Wang/fetcher",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ahoo-Wang/fetcher.git"
  },
  "bugs": {
    "url": "https://github.com/Ahoo-Wang/fetcher/issues"
  },
  "packageManager": "pnpm@10.15.1",
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.33.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "unplugin-dts": "1.0.0-beta.6",
    "vite": "^7.1.3",
    "vite-bundle-analyzer": "^1.2.3",
    "vitest": "^3.2.4"
  }
}
